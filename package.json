{
  "name": "react-notes",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "@cloudflare/wrangler": "^1.19.0",
    "concurrently": "^5.3.0",
    "date-fns": "^2.16.1",
    "esbuild": "^0.12.20",
    "excerpts": "^0.0.3",
    "fs-extra": "^10.0.0",
    "itty-router": "^2.4.2",
    "marked": "^1.2.5",
    "mkdirp": "^1.0.4",
    "nodemon": "^2.0.6",
    "object-assign": "^4.1.1",
    "resolve": "1.12.0",
    "rimraf": "^3.0.2",
    "sanitize-html": "^2.2.0",
    "serve": "^12.0.0",
    "source-map-explorer": "^2.5.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "prettier": "1.19.1"
  },
  "scripts": {
    "start": "concurrently \"npm run server:dev\" \"npm run bundler:dev\"",
    "start:prod": "concurrently \"npm run server:prod\" \"npm run bundler:prod\"",
    "server:dev": "cross-env NODE_ENV=development nodemon -- --conditions=react-server server",
    "server:prod": "cross-env NODE_ENV=production nodemon -- --conditions=react-server server",
    "bundler:dev": "cross-env NODE_ENV=development nodemon -- scripts/build.js",
    "bundler:prod": "cross-env NODE_ENV=production nodemon -- scripts/build.js",
    "prettier": "prettier --write **/*.js",
    "seed": "node ./scripts/seed.js"
  }
}
